<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.plantdiscover">
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@drawable/plant_photo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/plant_photo_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PlantDiscover">
        <activity android:name=".feature.menu.StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".feature.main.MainActivity"/>
    </application>

</manifest>